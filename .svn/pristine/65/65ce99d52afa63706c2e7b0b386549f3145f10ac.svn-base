#offnone, .offclose{
  display: block;
}
.offcanvas {
    position: fixed;
    z-index: 1000;
    display: none;
    transform: translate3d(0, 0, 0);
    transition: transform 800ms cubic-bezier(0.645, 0.045, 0.355, 1);
    background: #fff;
    height: 100%;
    overflow-x: scroll;
}


.offcanvas--left {
    top: 0;
    left: -100vw;
    width: 100vw;
    height: 100vh
}

.offcanvas--left--active {
    transform: translate3d(100vw, 0, 0)
}

.offcanvas--initialized {
    display: block;
    height: 100%;
}
