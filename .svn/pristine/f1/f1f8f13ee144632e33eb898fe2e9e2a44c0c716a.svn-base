/* loading Bar Css */
.bar {
  width: 100%;
    height: 20px;
    border: 1px solid #3b7398;
    border-radius: 3px;
    background-image: repeating-linear-gradient( -45deg, #3b7398, #3b7398 11px, #eee 10px, #eee 20px /* determines size */ );
    background-size: 28px 28px;
    animation: move .5s linear infinite;
}
 
@keyframes move {
  0% {
    background-position: 0 0;
  }
  100% {
    background-position: 28px 0;
  }
} 
/* end of Loading bar css */


