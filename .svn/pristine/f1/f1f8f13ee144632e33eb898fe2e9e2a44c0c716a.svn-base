 .hoteldetail-heading{
	margin-top: 20px;
	
}

.hoteldetail-heading ul{
	text-decoration: none;
	padding-left: 0px;
	
	
}
.hoteldetail-heading ul li{
	display: inline;
	padding: 0px 10px;
	
}

.hoteldetail-heading ul li a:hover,
.hoteldetail-heading ul li a:focus{ text-decoration: none;}

.hoteldetail-heading ul li a{
    display: inline-block;
    position: relative;
   /*  border-bottom: 3px solid #03a9f4 */
    
}
.hoteldetail-heading ul li a:before {
    content: '';
    display: block;
    position: absolute;
    left: 0;
    bottom: -3px;
    height: 3px;
    width: 0;
    transition: width 0s ease, background .5s ease;
}
.hoteldetail-heading ul li a:after {
    content: '';
    display: block;
    position: absolute;
    right: 0;
    bottom: -3px;
    height: 3px;
    width: 0;
    background: #D56100;
    transition: width .5s ease;
}
.hoteldetail-heading ul li a:hover:before {
    width: 100%;
    background: #D56100;
    transition: width .5s ease;
}
.hoteldetail-heading ul li a:hover:after {
    width: 100%;
    background: transparent;
    transition: all 0s ease;
}
.hotel-detail .scroll-header .hoteldetail-heading {
 text-align: center;
	
}


