  .mobileDeap-meal a,
  .dep-meal a ,
  .accordiontoggle{
  position: relative;
  
  
}
.mobileDeap-meal a::before,
.mobileDeap-meal a::after,
.dep-meal a::before,
.dep-meal a::after{
 right: -20px;
}
.accordiontoggle::before,
.accordiontoggle::after{
	right: 26px;
	
}
.mobileDeap-meal a::before,
.mobileDeap-meal a::after ,
.dep-meal a::before,
.dep-meal a::after,
.accordiontoggle::before,
.accordiontoggle::after{
  content: '';
  display: block;
  position: absolute;
  top: 50%; 
  width: 10px;
  height: 2px;
  margin-top: -1px;
  background-color: #1595D3;
  -webkit-transform-origin: 50% 50%;
  -ms-transform-origin: 50% 50%;
  transform-origin: 50% 50%;
  -webkit-transition: all 0.25s;
  transition: all 0.25s; 
}
.mobileDeap-meal a::before,
.dep-meal a::before ,
.accordiontoggle::before
{
  -webkit-transform: rotate(-90deg);
  -ms-transform: rotate(-90deg);
  transform: rotate(-90deg);
  opacity: 0;
}
.mobileDeap-meal a.collapsed::before,
.dep-meal a.collapsed::before,
.accordiontoggle.collapsed::before {
  -webkit-transform: rotate(0deg);
  -ms-transform: rotate(0deg);
  transform: rotate(0deg);
  opacity: 1;
}
.mobileDeap-meal a.collapsed::after,
.dep-meal a.collapsed::after,
.accordiontoggle.collapsed::after  {
  -webkit-transform: rotate(-90deg);
  -ms-transform: rotate(-90deg);
  transform: rotate(-90deg);
}
 
