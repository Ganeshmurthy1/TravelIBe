  white-space: nowrap;
}

rzslider span.base {
  width: 100%;
  height: 100%;
  padding: 0;
}

rzslider span.bar {
/*   z-index: 0;
  width: 100%;
  height: 100%;
  background: #fff; */
      width: 100%;
    height: 100%;
    z-index: 0;
    -webkit-border-radius: 1em;
    border-radius: 1em;
    background: -webkit-gradient(linear,left top,left bottom,color-stop(0,#fff),color-stop(1,#2FB3FA));
    background: -webkit-linear-gradient(top,#2FB3FA 0,#2FB3FA 100%);
    background: -moz-linear-gradient(top,#2FB3FA 0,#2FB3FA 100%);
    background: -o-linear-gradient(top,#2FB3FA 0,#2FB3FA 100%);
    background: -ms-linear-gradient(top,#2FB3FA 0,#2FB3FA 100%);
    background: linear-gradient(top,#2FB3FA 0,#2FB3FA 100%);
    box-shadow: inset 2px 2px 5px;
  
}

rzslider span.bar.selection {
     width: 0;
    z-index: 1;
    background: -webkit-gradient(linear,left top,left bottom,color-stop(0,#0082CB),color-stop(1,#0082CB));
    background: -webkit-linear-gradient(top,#0082CB 0,#0082CB 100%);
    background: -moz-linear-gradient(top,#0082CB 0,#0082CB 100%);
    background: -o-linear-gradient(top,#0082CB 0,#0082CB 100%);
    background: -ms-linear-gradient(top,#0082CB 0,#0082CB 100%);
    background: linear-gradient(top,#0082CB 0,#0082CB 100%);
    -webkit-box-shadow: none;
    box-shadow: none;
  
}

rzslider span.pointer {
      top: -10px;
    z-index: 2;
    width: 22px;
    height: 22px;
    cursor: pointer;
  background-color: #fff;
  -webkit-border-radius: 16px;
     -moz-border-radius: 16px;
          border-radius: 16px;
          box-shadow: 0px 0px 1px 0px #D56100;
}

rzslider span.pointer:after {
  position: absolute;
    top: 8px;
    left: 8px;
    width: 6px;
    height: 6px;
    background: #D56100;
  -webkit-border-radius: 4px;
     -moz-border-radius: 4px;
          border-radius: 4px;
  content: '';
}

rzslider span.pointer:hover:after {
  background-color: #67b700;
}

rzslider span.pointer.active:after {
  background-color: #67b700;
}

rzslider span.bubble {
  top: -32px;
  padding: 1px 3px 1px 3px;
  color: #67b700;
  cursor: default;
      display: none;
}

rzslider span.bubble.selection {
  top: 15px;
}

rzslider span.bubble.limit {
  /*color: #808080;*/
  display: none;

}


